                        <div class="row">
                            <div class="col-md-3 col-sm-3 col-xs-3">
                                <label class="control-label">
                                    Event Name <span class="text-danger">*</span>
                                </label>
                            </div>
                            <div class="col-md-9 col-sm-9 col-xs-9">
                                <input type="text" class="form-control" placeholder="Enter event name here" required />
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-3 col-sm-3 col-xs-3">
                                <label class="control-label">
                                    Location <span class="text-danger">*</span>
                                </label>
                            </div>
                            <div class="col-md-9 col-sm-9 col-xs-9">
                                <input type="text" class="form-control" id="autocomplete" placeholder="Enter your address" required />
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-3 col-sm-3 col-xs-3">
                                <label class="control-label">
                                    Start <span class="text-danger">*</span>
                                </label>
                            </div>
                            <div class="col-md-9 col-sm-9 col-xs-9 ">
                                <input type="text" class="form-control" id="start_date" required  />
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-3 col-sm-3 col-xs-3">
                                <label class="control-label">
                                    End <span class="text-danger">*</span>
                                </label>
                            </div>
                            <div class="col-md-9 col-sm-9 col-xs-9">
                                <input type="text" class="form-control"  id="end_date" required/>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-3 col-sm-3 col-xs-3">
                                <label class="control-label">
                                    Category <span class="text-danger">*</span>
                                </label>
                            </div>
                            <div class="col-md-9 col-sm-9 col-xs-9">
                                <select class="form-control" required>
                                    <option value>Select...</option>
                                    <option value="1">Option 1</option>
                                    <option value="2">Option 2</option>
                                    <option value="3">Option 3</option>
                                </select>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-3 col-sm-3 col-xs-3">
                                <label class="control-label">
                                    Free/Paid <span class="text-danger">*</span>
                                </label>
                            </div>
                            <div class="col-md-9 col-sm-9 col-xs-9">
                                <select class="form-control" required>
                                    <option value>Select...</option>
                                    <option value="1">Free</option>
                                    <option value="2">Paid</option>
                                </select>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-3 col-sm-3 col-xs-3">
                                <label class="control-label">
                                    Upload Image 
                                </label>
                            </div>
                            <div class="col-md-9 col-sm-9 col-xs-9">
                                <div class="custom-file">
                                    <input type="file" class="form-control" id="image" multiple accept='image/*'>
                                </div>
                                <br>
                                <img id="image_placeholder" src="images/image-placeholder.png" alt="preview" height="100" width="100" style="margin-top: 5px;">
                            </div>
                        </div>

                        <div class="row">
                                <div class="col-md-3 col-sm-3 col-xs-3">
                                    <label class="control-label">
                                        Event Description <span class="text-danger">*</span>
                                    </label>
                                </div>
                                <div class="col-md-9 col-sm-9 col-xs-9">
                                    <textarea class="form-control" rows="3" required></textarea>
                                </div>
                            </div>


                  
                </form>
